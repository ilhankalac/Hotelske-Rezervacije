<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Test</servlet-name>
        <servlet-class>Test</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>KlijentController</servlet-name>
        <servlet-class>Controllers.Registracija</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Controllers.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Logout</servlet-name>
        <servlet-class>Controllers.Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Klijenti</servlet-name>
        <servlet-class>Controllers.Klijenti</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditKlijent</servlet-name>
        <servlet-class>Controllers.EditKlijent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>KreiranjeHotela</servlet-name>
        <servlet-class>Controllers.KreiranjeHotela</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Test</servlet-name>
        <url-pattern>/Test</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>KlijentController</servlet-name>
        <url-pattern>/KlijentController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Logout</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Klijenti</servlet-name>
        <url-pattern>/Klijenti</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditKlijent</servlet-name>
        <url-pattern>/EditKlijent</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>KreiranjeHotela</servlet-name>
        <url-pattern>/KreiranjeHotela</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <dependency>
        <groupId>javax.servlet</groupId>
        <artifactId>javax.servlet-api</artifactId>
        <version>3.0.1</version>
    </dependency>
</web-app>
